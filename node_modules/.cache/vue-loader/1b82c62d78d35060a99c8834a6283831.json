 '1.910', active: 0, id: 22, type: '2' }\n        ]\n      ],\n      str: '',\n      childerArr: [],\n      // BetArr: [],\n      // price: undefined,\n      chooseArray: []\n    }\n  },\n  components: {\n    item2\n  },\n  methods: {\n    buy() {},\n    handleChange(value) {\n      console.log(value)\n    },\n    //当前选中的值,当前选中的行\n    onChange(item) {\n      this.str = ''\n      this.childerArr = this.$children.filter(\n        vm => vm.$options.name === 'items'\n      )\n      this.forin(item)\n      let strs = this.str\n      console.log(`当前选中:${strs}`)\n      this.submit(item)\n    },\n    submit(item) {\n      //先提交数据\n        this.chooseArray.push({\n          id: item.id,\n          str: this.str,\n          nums: this.nums\n        })\n        this.str = ''\n        this.nums = 0\n        this.childerArr = this.$children.filter(\n          vm => vm.$options.name === 'items'\n        )\n    },\n    forin(item) {\n      console.log(item)\n      if (item.type == 1) {\n        this.str = item.name + ',-'\n      } else {\n        this.str = '-,' + item.name\n      }\n    },\n    ItemDelete(index, item) {\n      console.log(item)\n      if (item) {\n        for (let i = 0; i < this.chooseArray.length; i++) {\n          if (this.chooseArray[i].id === item.id) {\n            this.chooseArray.splice(i, 1)\n          }\n        }\n        return\n      }\n      this.chooseArray.splice(index, 1)\n    }\n  }\n}\n</script>\n\n<style scoped>\n.itemp {\n  padding: 30px;\n  padding-top: 0px;\n}\n.flex {\n  display: flex;\n  align-items: center;\n}\n.el-input {\n  width: 50%;\n}\n</style>\n"]}]}