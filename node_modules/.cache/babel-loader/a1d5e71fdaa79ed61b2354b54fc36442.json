 * this.nums;\n            // }\n        },\n        methods: {\n            buy() {\n\n            },\n            handleChange(value) {\n                console.log(value)\n            },\n            onChange(content) {\n                console.log(content)\n                this.str = '';\n                this.childerArr = this.$children.filter(\n                    vm => vm.$options.name === 'items'\n                );\n                for (let i = 0; i < this.childerArr.length; i++) {\n                    let vm = this.$children[i];\n                    //当前选中的字符串序列\n                    this.forin(vm.activeItem, i);\n                }\n                this.str = this.str.substr(0, this.str.length - 1);\n                let strs = this.str;\n                console.log(`当前选中:${strs}`);\n                this.submit()\n            },\n            submit() {\n                //先提交数据\n                if (this.str !== '-,-,-,-,-,-,-,-,-,-') {\n                    this.chooseArray.push({\n                        str: this.str,\n                        nums: this.nums,\n                    });\n                    this.str = '';\n                    this.nums = 0;\n                    this.childerArr = this.$children.filter(\n                        vm => vm.$options.name === 'items'\n                    );\n                    //清空\n                    this.delete(this.childerArr)\n                }\n\n            },\n            forin(arr) {\n                if (arr.length === 0) {\n                    this.str += '-,'\n                } else {\n                    for (let j = 0; j < arr.length; j++) {\n                        this.str += arr[j] + ','\n                        // this.BetArr.push(arr[j])\n                    }\n                }\n            },\n            // //清空\n            delete(arr) {\n                for (let i = 0; i < arr.length; i++) {\n                    let vm = arr[i];\n                    vm.activeItem = [];\n                    vm.indexs = undefined;\n                }\n                this.BetArr = [];\n                this.str = ''\n            },\n            ItemDelete(index,contentIndex) {\n                if(contentIndex){\n                    for (let i=this.chooseArray.length; i<0;i--) {\n                        if(this.chooseArray[i].str.indexOf(contentIndex)>-1){\n                            console.log(i)\n                            this.chooseArray.splice(i, 1);\n                            return false;\n                        }\n                    }\n                    return;\n                }\n                this.chooseArray.splice(index, 1)\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .itemp {\n        padding: 30px;\n        padding-top: 0px;\n    }\n    .flex{\n        display: flex;\n        align-items: center;\n\n    }\n    .el-input{\n        width: 50%;\n    }\n</style>"],"sourceRoot":"src/components/item"}]}